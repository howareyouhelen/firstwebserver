<head>
  <title>TinyAPP</title>
  <link rel="shortcut icon" href="/favicon.ico" type="image/png">
</head>
<body>
  <h1><font colour="white">Tiny App </h1>
  <% if (user) { %>
  <p><%= user.id %> </p>
  <form method="POST" action="/logout">
    <input type="submit" value="logout">
  </form>

 <% } else { %>
  <h2>
  <p style="display: inline-block;"><%= user %></p>
  </h2>

  <a href="http://localhost:1234/register">Sign up</a><br>
  <a href="http://localhost:1234/login">Login</a>

  <% } %>
</body>